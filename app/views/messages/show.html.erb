<table border>
  <tr>
    <% @message_headers.each do |header| %>
      <th>
        <%= header %>
      </th>
    <% end %>
  </tr>

  <% @message_rows.each do |row| %>
    <tr>
      <% row.each do |column| %>
        <td>
          <%= column %>
        </td>
      <% end %>
    </tr>
  <% end %>
</table>
